{
	"sap.app": {
		"id": "sap.f.cardsdemo.showCard.noHeaderDetails",
		"type": "card",
		"title": "Sample of an Snack Card",
		"subTitle": "Sample of an Snack Card",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"shortTitle": "A short title for this Card",
		"info": "Additional information about this Card",
		"description": "A long description for this Card",
		"tags": {
			"keywords": [
				"Object",
				"Card",
				"Sample",
				"Snack"
			]
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://switch-classes"
		}
	},
	"sap.card": {
		"type": "Object",
		"data": {
			"request": {
				"url": "{{mainDestinations.myDestination}}/Orders",
				"parameters": {
					"$format": "json",
					"$filter": "OrderID eq {parameters>/orderId/value}"
				}
			},
			"path": "/value/0"
		},
		"configuration": {
			"useMainDestinations": true,
			"parameters": {
				"orderId": {
					"value": 10249
				}
			}
		},
		"header": {
			"data": {
				"path": "/value/0"
			},
			"title": "Card with hidden header",
			"visible": false
		},
		"content": {
			"groups": [
				{
					"items": [
						{
							"label": "Name",
							"value": "{ShipName}"
						},
						{
							"label": "Address",
							"value": "{ShipAddress}"
						},
						{
							"label": "Country",
							"value": "{ShipCountry}"
						},
						{
							"label": "City",
							"value": "{ShipCity}"
						},
						{
							"label": "Postal code",
							"value": "{ShipPostalCode}"
						}
					]
				}
			]
		}
	}
}